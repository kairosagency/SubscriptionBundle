{
    "name": "kairos/subscriptionbundle",
    "type": "library",
    "license": "LGPL-3.0",
    "description": "subscription integration bundle for symfony2. This bundle is aimed at being generic and include different subscription providers.",
    "keywords": ["subscription", "payment", "subscription payment", "braintree", "symfony2", "bundle"],
    "homepage": "https://github.com/kairosagency/SubscriptionBundle",
    "authors": [
        {
            "name": "t0k4rt",
            "homepage": "https://github.com/t0k4rt",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "symfony/framework-bundle": "~2.1",
        "symfony/form": "~2.1",
        "braintree/braintree_php" : "~2.27"
    },
    "require-dev": {
        "twig/twig": "~1.5",
        "doctrine/doctrine-bundle": "*",

        "symfony/class-loader": "~2.3",
        "symfony/yaml": "~2.3",
        "symfony/finder": "~2.3",

        "phpunit/phpunit": "~4.2",
        "patchwork/dumper": "~1.2"
    },
    "autoload": {
        "psr-0": { "Kairos\\SubscriptionBundle": "" }
    },
    "target-dir": "Kairos/SubscriptionBundle"
}
